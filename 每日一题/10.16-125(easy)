//最后一个测试用例超出内存限制
bool isPalindrome(string s) {
        string str;
        int size=s.size();
        int i;
        for(i=0;i<size;i++){
            if(('a'<=s[i]&&s[i]<='z')||('A'<=s[i]&&s[i]<='Z')||('0'<=s[i]&&s[i]<='9')){
            	if('A'<=s[i]&&s[i]<='Z')
            		s[i]=tolower(s[i]);
            	str=str+s[i];
			}
        }
        int length=str.size();
        for(i=0;i<length/2;i++){
            if(str[i]!=str[length-i-1])
                break;
        }
        if(i<length/2) return false;
        return true;
        
   }
